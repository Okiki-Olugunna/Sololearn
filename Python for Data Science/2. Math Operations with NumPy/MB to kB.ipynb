     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "data_mb = np.array([6, 3, 10, 143])\n",
    "\n",
    "data_kb = data_mb * 1024\n",
    "\n",
    "print(data_kb.max())"
   ]
  }
